    "husky": "^8.0.3",
    "prop-types": "^15.8.1",
    "storybook": "^7.1.0",
    "webpack": "^5.88.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
